  <div class="mt-5">
    <h2>New book</h2>
    <%= form_with model: @newbook do |f| %>
      <h4>Title</h4>
        <%= f.text_field :title %>
      <h4>Opinion</h4>
        <%= f.text_area :body %>
      <div class="mt-3">
        <p><%= f.submit 'Create Book', class: 'btn btn-success' %></P>
      </div>
    <% end %>
</div>